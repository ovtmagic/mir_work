#!/bin/bash

mkdir /tmp/mir

cp /home/octavio/Dropbox/MIR/Datasets/*arff /tmp/mir

tar zxvf /home/octavio/Dropbox/MIR/Datasets/clasica.tgz
tar zxvf /home/octavio/Dropbox/MIR/Datasets/jazz.tgz
tar zxvf /home/octavio/Dropbox/MIR/Datasets/kar.tgz

cp /home/octavio/Dropbox/MIR/cosas/work2/clasica.csv .
cp /home/octavio/Dropbox/MIR/cosas/work2/jazz.csv .
cp /home/octavio/Dropbox/MIR/cosas/work2/kar.csv .
